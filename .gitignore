# Ignore bundler config.
/backend/.bundle

# Ignore all logfiles and tempfiles.
/backend/src/log/*
/backend/src/tmp/*
!/backend/src/log/.keep
!/backend/src/tmp/.keep

/node_modules
/yarn-error.log

/backend/.byebug_history

# Ignore uploads data
/backend/src/public/uploads

# Ignore IDE
.idea

# Ignore OS
.DS_Store

# Ignore Docker folder structure
/db
/pgadmin